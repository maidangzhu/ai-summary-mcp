{
  "name": "ai-collaboration-archive",
  "version": "3.0.0",
  "description": "AI协作档案分析器 - Monorepo",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "start": "pnpm -r start",
    "clean": "pnpm -r clean",
    "db:generate": "prisma generate --schema=./packages/db/prisma/schema.prisma",
    "db:push": "prisma db push --schema=./packages/db/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./packages/db/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=./packages/db/prisma/schema.prisma"
  },
  "workspaces": [
    "packages/*"
  ],
  "keywords": [
    "mcp",
    "ai",
    "analysis",
    "collaboration",
    "archive",
    "tech-stack",
    "business-analysis",
    "problem-classification",
    "solution-extraction",
    "deepseek",
    "openai",
    "claude"
  ],
  "author": "zhujianye",
  "license": "MIT",
  "packageManager": "pnpm@10.8.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/ai-collaboration-archive-analyzer.git"
  },
  "homepage": "https://github.com/your-username/ai-collaboration-archive-analyzer#readme",
  "bugs": {
    "url": "https://github.com/your-username/ai-collaboration-archive-analyzer/issues"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "prisma": "^6.15.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "typescript": "^5.9.2"
  }
}
